{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.8",
        "laravel/framework": "^6.4",
        "laravelcollective/html": "6.0.*",
        "cviebrock/eloquent-sluggable": "^6.0.0",
        "simplepie/simplepie": "^1.4",
        "propaganistas/laravel-phone": "^2.8",
        "laravel/scout": "3.0.0",
        "algolia/algoliasearch-client-php": "^1.12",
        "pusher/pusher-php-server": "^3.0",
        "laravel/browser-kit-testing": "^1.0",
        "andreasindal/laravel-markdown": "^1.1",
        "laravel/cashier": "~10.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "laravel/tinker": "^1.0.8",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/horizon": "^3.2.7",
        "fideloper/proxy": "^4.0",
        "pda/pheanstalk": "~4.0",
        "spatie/laravel-sitemap": "^5.4",
        "spatie/crawler": "^4.1.0",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "nesbot/carbon": "2.0.0-beta.2 as 1.25.0",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "^4.0",
        "laravel/dusk": "^5.5",
        "facade/ignition": "^1.4",
        "nunomaduro/collision": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/BrowserKitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}